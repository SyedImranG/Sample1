@model MembershipDEMO.Models.AllroleandUser
@{
    ViewBag.Title = "DisplayAllUserroles";
}

<h2>DisplayAllUserroles</h2>

<table border="1">
    <tr>
        <td>User Name</td>
        <td>Role Name</td>
    </tr>

    @foreach (var item in Model.AllDetailsUserlist)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.RoleName</td>
        </tr>
    }

</table>
